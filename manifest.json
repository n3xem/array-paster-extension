{
  "manifest_version": 3,
  "name": "Array Paster",
  "version": "1.0",
  "description": "複数の文字列をキューに登録し、右クリックメニューから貼り付けるための拡張機能",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["contextMenus", "storage", "activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  }
}
